{
  "env" : {
    "node" : true
  },
  "globals": {
        "fd": true,
        "sap": true,
        "jQuery" : true,
        "$"      : false,
		"_"       : false,
        "alert"  : false,
        "prompt" : false,
        "confirm": false,
        "window" : false,
        "loadXMLString": false,
        "OData":   false,
		"joint":   false,
        "document" : false,
        "CodeMirror" : false
  },

  "rules" : {
    "semi": [2, "always"],
    "strict": 0,
    "quotes": [1, "double", "avoid-escape"],
    "no-use-before-define": 0,
    "curly": 0,
    "no-underscore-dangle": 0,
    "no-lonely-if": 1,
    "no-shadow": 0,
    "no-unused-vars": [2, {"vars" : "all", "args" : "after-used"}],
    "no-mixed-requires": 0,
    "space-infix-ops": 0,
    "key-spacing": 0,
    "no-multi-spaces": 0,
	"no-undef": 2,
	"no-trailing-spaces": 0,
  "no-alert": 0,
  "comma-dangle": 0,
  "no-unused-vars": 0,
  "semi-spacing": 0,
  "no-mixed-spaces-and-tabs": 0,
  "eqeqeq": "2",
  "no-extend-native": 0,
  "camelcase": 0,
  "dot-notation": 0,
  "no-eval": 0,
  "no-loop-func": 0,
  "doublequote": 0,
  "quotes": 0,
  "eol-last": 0,
  "space-unary-ops": 0,
  //"no-redeclare": 0,
  "no-lonely-if": 0,
  "no-constant-condition": 0,
  "comma-spacing": 0,
  "new-cap" : 0,
  }
}
