<core:View
    xmlns:semantic="sap.m.semantic"
    xmlns:core="sap.ui.core"
    xmlns:smartfilterbar="sap.ui.comp.smartfilterbar"
    xmlns:m="sap.m"
    xmlns:smarttable="sap.ui.comp.smarttable"
    xmlns:table="sap.ui.table">

    <semantic:FullscreenPage
        title=""
        showNavButton="">
        <semantic:semanticControls>
            <semantic:SemanticButton
                type="SaveAsTile">
            </semantic:SemanticButton>
            <semantic:SemanticSelect>
                <semantic:items>
                    <core:ListItem
                        text="one">
                    </core:ListItem>
                    <core:ListItem
                        text="one">
                    </core:ListItem>
                    <core:ListItem
                        text="two">
                    </core:ListItem>
                </semantic:items>
            </semantic:SemanticSelect>
        </semantic:semanticControls>
        <semantic:content>
            <smartfilterbar:SmartFilterBar
                id=""
                entityType="">
                <smartfilterbar:controlConfiguration>
                    <smartfilterbar:ControlConfiguration
                        key="CompanyCode">
                    </smartfilterbar:ControlConfiguration>
                </smartfilterbar:controlConfiguration>
            </smartfilterbar:SmartFilterBar>
            <m:IconTabBar
                expandable="">
                <m:content>
                    <smarttable:SmartTable
                        entitySet=""
                        tableType="AnalyticalTable"
                        header=""
                        smartFilterId="">
                        <smarttable:items>
                            <table:AnalyticalTable
                                selectionMode="">
                                <table:columns>
                                    <table:AnalyticalColumn>
                                        <table:label>
                                            <m:Label
                                                text="{i18n>CompanyCode}">
                                            </m:Label>
                                        </table:label>
                                        <table:template>
                                            <m:Text
                                                text="CompanyCode">
                                            </m:Text>
                                        </table:template>
                                    </table:AnalyticalColumn>
                                </table:columns>
                            </table:AnalyticalTable>
                        </smarttable:items>
                    </smarttable:SmartTable>
                </m:content>
                <m:items>
                    <m:IconTabFilter
                        text="All"
                        key="All">
                    </m:IconTabFilter>
                    <m:IconTabSeparator>
                    </m:IconTabSeparator>
                    <m:IconTabFilter
                        text="New"
                        key=""
                        icon="sap-icon://bookmark"
                        iconColor="Neutral">
                    </m:IconTabFilter>
                </m:items>
            </m:IconTabBar>
        </semantic:content>
    </semantic:FullscreenPage>
</core:View>