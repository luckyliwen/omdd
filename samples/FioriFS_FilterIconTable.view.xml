<core:View
    controllerName="test"
    xmlns:semantic="sap.m.semantic"
    xmlns:smartfilterbar="sap.ui.comp.smartfilterbar"
    xmlns:core="sap.ui.core"
    xmlns="sap.m"
    xmlns:smarttable="sap.ui.comp.smarttable"
    xmlns:table="sap.ui.table">

    <semantic:FullscreenPage
        title=""
        showNavButton="">
        <semantic:content>
            <smartfilterbar:SmartFilterBar
                id="smartFilterBar"
                entityType="PaytProposal"
                persistencyKey="fin.ap.paymentproposal.revise.FilterBar"
                reset="doReload"
                initialise="onInitSmartFilterBar"
                search="storeInnerAppState">
                <smartfilterbar:customData>
                    <core:CustomData
                        key="defaultDropDownDisplayBehaviour"
                        value="descriptionOnly">
                    </core:CustomData>
                    <core:CustomData
                        key="defaultTokenDisplayBehaviour"
                        value="idAndDescription">
                    </core:CustomData>
                </smartfilterbar:customData>
                <smartfilterbar:controlConfiguration>
                    <smartfilterbar:ControlConfiguration
                        key="RunDate"
                        index="0"
                        controlType="date"
                        hasTypeAhead="false"
                        groupId="_BASIC">
                    </smartfilterbar:ControlConfiguration>
                    <smartfilterbar:ControlConfiguration
                        key="RunID"
                        index="1"
                        groupId="_BASIC">
                    </smartfilterbar:ControlConfiguration>
                    <smartfilterbar:ControlConfiguration
                        key="CreatedBy"
                        index="2"
                        groupId="_BASIC">
                    </smartfilterbar:ControlConfiguration>
                </smartfilterbar:controlConfiguration>
            </smartfilterbar:SmartFilterBar>
            <IconTabBar
                expandable="">
                <content>
                    <smarttable:SmartTable
                        id="SmartTableList"
                        entitySet="PaytProposalSet"
                        smartFilterId="smartFilterBar"
                        tableType="AnalyticalTable"
                        class="sapFioriListReportSmartTable"
                        header="Proposal"
                        ignoredFields=""
                        enableAutoBinding="true"
                        beforeRebindTable="onBeforeRebindTable"
                        afterVariantApply="storeInnerAppState">
                        <smarttable:items>
                            <table:AnalyticalTable
                                id="list"
                                visibleRowCountMode="Auto"
                                rowHeight="32"
                                allowColumnReordering="true"
                                showColumnVisibilityMenu="false"
                                selectionMode="Single"
                                enableColumnFreeze="false"
                                enableCellFilter="false"
                                selectionBehavior="RowSelector"
                                busyIndicatorDelay="400">
                                <table:columns>
                                    <table:AnalyticalColumn
                                        id="RunDate"
                                        hAlign="Begin"
                                        sortProperty="RunDate"
                                        leadingProperty="RunDate"
                                        filterProperty="RunDate"
                                        grouped="false"
                                        summed="false"
                                        inResult="true"
                                        showFilterMenuEntry="false"
                                        autoResizable="true"
                                        tooltip="{/#PaytProposal/RunDate/@sap:label}">
                                        <table:label>
                                            <Label
                                                text="{/#PaytProposal/RunDate/@sap:label}">
                                            </Label>
                                        </table:label>
                                        <table:template>
                                            <Text
                                                text="{RunDate}"
                                                wrapping="false">
                                            </Text>
                                        </table:template>
                                        <table:customData>
                                            <core:CustomData
                                                key="p13nData"
                                                value="\{'columnKey': 'RunDate'}">
                                            </core:CustomData>
                                        </table:customData>
                                    </table:AnalyticalColumn>
                                </table:columns>
                            </table:AnalyticalTable>
                        </smarttable:items>
                    </smarttable:SmartTable>
                </content>
                <items>
                    <IconTabFilter
                        text="All"
                        key="All">
                    </IconTabFilter>
                    <IconTabSeparator>
                    </IconTabSeparator>
                    <IconTabFilter
                        text="New"
                        key=""
                        icon="sap-icon://bookmark"
                        iconColor="Neutral">
                    </IconTabFilter>
                </items>
            </IconTabBar>
        </semantic:content>
    </semantic:FullscreenPage>
</core:View>